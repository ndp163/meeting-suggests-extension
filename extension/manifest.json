{
  "manifest_version": 3,
  "name": "Audio Capture Extension",
  "version": "1.0",
  "description": "Captures audio from the current tab",
  "permissions": ["tabs", "scripting", "activeTab", "tabCapture", "sidePanel"],
  "action": {
    "default_title": "Open meeting AI"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}
